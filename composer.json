{
    "name": "labsmobile/sms-php",
    "description": "Send SMS messages through the LabsMobile platform and the PHP library.",
    "type": "library",
    "license": "MIT",
    "keywords": [
      "php",
      "composer",
      "sms",
      "httpclient",
      "sms-api",
      "sms-messages",
      "sms-client",
      "sms-notifications",
      "smsapi",
      "sms-php",
      "labsmobile"
    ],
    "autoload": {
        "psr-4": {
            "Labsmobile\\SmsPhp\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
          "Labsmobile\\SmsPhp\\Tests\\": "tests/"
      }
    },
    "authors": [
        {
            "name": "LabsMobile",
            "email": "support@labsmobile.com",
            "homepage": "https://www.labsmobile.com/"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "require": {
        "php": "^7.0.0",
        "guzzlehttp/guzzle": "^7.8",
        "ext-json": "*"
    }
}
